<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
		    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <RadialGradientBrush x:Key="GreenEllipseBrush" Center="0.5,0.5" RadiusX="0.5" RadiusY="0.5" GradientOrigin="0.2,0.2">
        <GradientStop Color="#FFFFFFFF" Offset="0"/>
        <GradientStop Color="#FF00FF00" Offset="0.5"/>
        <GradientStop Color="#FF00BB00" Offset="1"/>
    </RadialGradientBrush>

    <RadialGradientBrush x:Key="YellowEllipseBrush" Center="0.5,0.5" RadiusX="0.5" RadiusY="0.5" GradientOrigin="0.2,0.2">
        <GradientStop Color="#FFFFFFFF" Offset="0"/>
        <GradientStop Color="#FFFFFF00" Offset="0.5"/>
        <GradientStop Color="#FFBBBB00" Offset="1"/>
    </RadialGradientBrush>

    <RadialGradientBrush x:Key="RedEllipseBrush" Center="0.5,0.5" RadiusX="0.5" RadiusY="0.5" GradientOrigin="0.2,0.2">
        <GradientStop Color="#FFFFFFFF" Offset="0"/>
        <GradientStop Color="#FFFF0000" Offset="0.5"/>
        <GradientStop Color="#FFBB0000" Offset="1"/>
    </RadialGradientBrush>

    <RadialGradientBrush x:Key="GrayEllipseBrush" Center="0.5,0.5" RadiusX="0.5" RadiusY="0.5" GradientOrigin="0.2,0.2">
        <GradientStop Color="#FFFFFFFF" Offset="0"/>
        <GradientStop Color="#FF666666" Offset="0.5"/>
        <GradientStop Color="#FF333333" Offset="1"/>
    </RadialGradientBrush>
</ResourceDictionary>