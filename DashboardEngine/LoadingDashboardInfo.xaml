      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  
  <Grid>
    <ListBox x:Name="loadedAssemblies" ItemsSource="{Binding ElementName=currentPage, Path=LoadedAssemblies}"/>
  </Grid>
</Page>
